# Memcached installation
curl -O http://memcached.googlecode.com/files/memcached-1.4.5.tar.gz
tar -xzf memcached-1.4.5.tar.gz
cd memcached-1.4.5
./configure
make
sudo make install
cd ..
rm -fr memcached-1.4.5/
rm memcached-1.4.5.tar.gz


# Install python binding for memcached.
curl -O ftp://ftp.tummy.com/pub/python-memcached/python-memcached-latest.tar.gz
tar -zxf python-memcached-latest.tar.gz
cd python-memcached-1.47/
python setup.py install
cd ..
rm -fr python-memcached-1.47/
rm python-memcached-latest.tar.gz



# Start memcached server
memcached -d -m 512 -l 127.0.0.1 -p 11211 -u nobody



# ttdashboard installation

# This installation file pretend that you have a mysql server on your computer.

curl -O http://dl.dropbox.com/u/16682487/dump.sql

virtualenv ttdashboard

cd ttdashboard

source bin/activate

easy_install django==1.3

git clone git-croco@free2.projectlocker.com:ttdashboard.git

cd ttdashboard

mv settings.py.default settings.py

vim settings.py
# Set your database configurations.

python manage.py runserver

# Now you can open http://127.0.0.1:8000/ and it should work.
